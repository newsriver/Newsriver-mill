{
    "id": "/newsriver/newsriver-mill",
    "cmd": null,
    "cpus": 0.5,
    "mem": 768,
    "disk": 0,
    "instances": 1,
    "fetch": [{
        "uri": "file:///etc/docker/docker-grc-auth.tar.gz"
    }],
    "container": {
        "type": "DOCKER",
        "volumes": [],
        "docker": {
            "image": "gcr.io/newsriver-io/newsriver-mill:1",
            "network": "HOST",
            "privileged": false,
            "parameters": [],
            "forcePullImage": true
        }
    },
    "healthChecks": [{
        "path": "/",
        "protocol": "HTTP",
        "portIndex": 0,
        "gracePeriodSeconds": 90,
        "intervalSeconds": 60,
        "timeoutSeconds": 20,
        "maxConsecutiveFailures": 3,
        "ignoreHttp1xx": false
    }],
    "args": [
        "--threads=50",
        "--batch=50",
        "--queue=100"
    ],
    "acceptedResourceRoles": [
        "slave_public"
    ]
}
